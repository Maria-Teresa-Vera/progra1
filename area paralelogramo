#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;

void main()
{

float altura,base,area;


cout<<"ingrese altura";

cin>> altura;

cout<<"ingrese base";

cin>>base;

area=(base*altura);

cout<< "el area es" << area;

getch();

}
