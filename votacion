#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;
void main ()
{
	int edad;
	cout<<"ingrese edad: ";
	cin>>edad;
	if (edad>=18)
	cout<<"puede votar"<<endl;

	else
		cout<<"no puede votar";

	getch();
}
