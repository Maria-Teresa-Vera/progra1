#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int i,n,suma;
	cout<<"Ingrese numeros enteros:";
	cin>>n;
	suma=0;
	for (i=1;i<=n;i++)
		suma=suma+i;
		cout<<"el resultado es:"<<suma;
	getch();
}
